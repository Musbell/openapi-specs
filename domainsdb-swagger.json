        {
            "name": "default",
            "description": "Default namespace"
        },
        {
            "name": "info/",
            "description": "TLD Statistics"
        },
        {
            "name": "domains/",
            "description": "Domains Database"
        }
    ],
    "definitions": {
        "Zone stats": {
            "allOf": [
                {
                    "$ref": "#/definitions/Response Parameters"
                },
                {
                    "properties": {
                        "statistics": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Zone statistics"
                            }
                        }
                    },
                    "type": "object"
                }
            ]
        },
        "Response Parameters": {
            "required": [
                "time"
            ],
            "properties": {
                "total": {
                    "type": "integer",
                    "description": "Total days reported"
                },
                "time": {
                    "type": "string",
                    "description": "Time took"
                },
                "next_page": {
                    "type": "string",
                    "description": "Next page id"
                }
            },
            "type": "object"
        },
        "Zone statistics": {
            "required": [
                "date",
                "dec",
                "inc",
                "total",
                "zone"
            ],
            "properties": {
                "zone": {
                    "type": "string",
                    "description": "TLD name"
                },
                "date": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Report Date"
                },
                "dec": {
                    "type": "integer",
                    "description": "Increase"
                },
                "inc": {
                    "type": "integer",
                    "description": "Decrease"
                },
                "total": {
                    "type": "integer",
                    "description": "Total number of records"
                }
            },
            "type": "object"
        },
        "Zone info": {
            "required": [
                "type",
                "zone"
            ],
            "properties": {
                "zone": {
                    "type": "string",
                    "description": "TLD name"
                },
                "type": {
                    "type": "string",
                    "description": "TLD type"
                },
                "includes": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "Sub TLDs included"
                    }
                },
                "in_bundles": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "Sub TLDs included"
                    }
                },
                "description": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "Sub TLDs included"
                    }
                }
            },
            "type": "object"
        },
        "API Key Info": {
            "properties": {
                "success": {
                    "type": "string",
                    "description": "State"
                },
                "license": {
                    "type": "string",
                    "description": "State"
                },
                "item_id": {
                    "type": "string",
                    "description": "State"
                },
                "item_name": {
                    "type": "string",
                    "description": "State"
                },
                "expires": {
                    "type": "string",
                    "description": "State"
                },
                "customer_name": {
                    "type": "string",
                    "description": "State"
                },
                "customer_email": {
                    "type": "string",
                    "description": "State"
                },
                "license_limit": {
                    "type": "string",
                    "description": "State"
                },
                "price_id": {
                    "type": "string",
                    "description": "State"
                }
            },
            "type": "object"
        },
        "Search Results": {
            "allOf": [
                {
                    "$ref": "#/definitions/Response Parameters"
                },
                {
                    "properties": {
                        "domains": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/Domains"
                            }
                        }
                    },
                    "type": "object"
                }
            ]
        },
        "Domains": {
            "properties": {
                "domain": {
                    "type": "string",
                    "description": "Domain name"
                },
                "create_date": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Domain creation date"
                },
                "update_date": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Domain last update date"
                },
                "country": {
                    "type": "string",
                    "description": "Hosting Country"
                },
                "isDead": {
                    "type": "string",
                    "description": "If Domain is Dead"
                },
                "A": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "A record"
                    }
                },
                "NS": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "NS record"
                    }
                },
                "CNAME": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "CNAME record"
                    }
                },
                "MX": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/MX records"
                    }
                },
                "TXT": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "TXT record"
                    }
                }
            },
            "type": "object"
        },
        "MX records": {
            "properties": {
                "exchange": {
                    "type": "string",
                    "description": "MX domain"
                },
                "priority": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "Update model": {
            "properties": {
                "date": {
                    "type": "string",
                    "description": "Update date"
                },
                "added": {
                    "type": "string",
                    "description": "Updates added list"
                },
                "added_download": {
                    "type": "string",
                    "description": "Download link for added"
                },
                "deleted": {
                    "type": "string",
                    "description": "Updates deleted list"
                },
                "deleted_download": {
                    "type": "string",
                    "description": "Download link for deleted"
                }
            },
            "type": "object"
        }
    },
    "responses": {
